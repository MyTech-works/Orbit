    <style>
        #categoryModal, #orderStatusModal, #divisionsModal {
            position: absolute;
            top: 10px;
            right: 100px;
            bottom: 0;
            left: 0;
            z-index: 10040;
            overflow: auto;
            overflow-y: auto;
        }

        #scroll-to-top {
            position: fixed;
            right: 10px;
            bottom: 100px;
            cursor: pointer;
            width: 45px;
            height: 45px;
            background-color: #2874f0;
            text-indent: -9999px;
            display: none;
            -webkit-border-radius: 60px;
            -moz-border-radius: 60px;
            border-radius: 60px;
            z-index: 99999;
        }

            #scroll-to-top span {
                position: absolute;
                top: 50%;
                left: 50%;
                margin-left: -8px;
                margin-top: -12px;
                height: 0;
                width: 0;
                border: 8px solid transparent;
                border-bottom-color: #ffffff;
            }

            #scroll-to-top:hover {
                background-color: #e74c3c;
                opacity: 1;
                filter: "alpha(opacity=100)";
                -ms-filter: "alpha(opacity=100)";
            }

        .rainbow-progress {
            height: 5px;
            width: 100%;
            border-radius: 3px;
            background: linear-gradient(90deg, red 0%, yellow 15%, lime 30%, cyan 50%, blue 65%, magenta 80%, red 100%);
            background-size: 200%;
            animation: moveGradient 5s linear infinite;
        }

        @@keyframes moveGradient {
            0% {
                background-position: 0% 0%;
            }

            100% {
                background-position: -200% 0%;
            }
        }

        .hide-div {
            display: none !important;
        }
    </style>
</head>
<body>
